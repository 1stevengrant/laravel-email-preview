{
    "name": "ghijk/laravel-email-preview",
    "description": "A Laravel package for capturing and previewing emails in a web UI for staging/testing environments",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Steven Grant",
            "email": "steven@ghijk.nz"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/mail": "^10.0|^11.0|^12.0",
        "symfony/mailer": "^6.0|^7.0",
        "symfony/mime": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Ghijk\\EmailPreview\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ghijk\\EmailPreview\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0",
        "laravel/pint": "^1.0",
        "rector/rector": "^2.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "lint": "vendor/bin/pint",
        "lint-check": "vendor/bin/pint --test",
        "rector": "vendor/bin/rector",
        "rector-check": "vendor/bin/rector --dry-run"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ghijk\\EmailPreview\\EmailPreviewServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
